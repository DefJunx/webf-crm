{
   "name": "webformat-crm",
   "version": "1.0.0",
   "description": "Simple CRM",
   "main": "build/index.js",
   "engines": {
      "node": "12.16.1",
      "npm": "6.13.4"
   },
   "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "start": "node .",
      "dev": "nodemon",
      "build": "tsc",
      "prestart": "npm run typeorm:run",
      "typeorm": "node -r ts-node/register ./node_modules/typeorm/cli.js",
      "typeorm:migrate": "npm run typeorm migration:generate -- -n",
      "typeorm:run": "npm run typeorm migration:run",
      "typeorm:drop": "npm run typeorm schema:drop"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/DefJunx/webf-crm.git"
   },
   "keywords": [
      "webformat",
      "crm"
   ],
   "author": "Daniele Cencig",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/DefJunx/webf-crm/issues"
   },
   "eslintConfig": {
      "parser": "@typescript-eslint/parser",
      "plugins": [
         "@typescript-eslint",
         "prettier"
      ],
      "extends": [
         "eslint:recommended",
         "plugin:@typescript-eslint/recommended",
         "prettier"
      ],
      "rules": {
         "prettier/prettier": "error"
      },
      "ignorePatterns": [
         "public",
         "src/migration",
         "ormconfig.js"
      ]
   },
   "homepage": "https://github.com/DefJunx/webf-crm#readme",
   "dependencies": {
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "helmet": "^4.1.0",
      "morgan": "^1.10.0",
      "mysql2": "^2.1.0",
      "reflect-metadata": "^0.1.13",
      "ts-node": "^9.0.0",
      "typeorm": "^0.2.25"
   },
   "devDependencies": {
      "@types/cors": "^2.8.7",
      "@types/express": "^4.17.8",
      "@types/helmet": "0.0.48",
      "@types/morgan": "^1.9.1",
      "@types/node": "^14.6.4",
      "@typescript-eslint/eslint-plugin": "^4.0.1",
      "@typescript-eslint/parser": "^4.0.1",
      "eslint": "^7.8.1",
      "eslint-config-prettier": "^6.11.0",
      "eslint-plugin-prettier": "^3.1.4",
      "nodemon": "^2.0.4",
      "prettier": "^2.1.1",
      "typescript": "^4.0.2"
   }
}
